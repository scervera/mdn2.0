/* These are the styling for the prayer marquee */

ul.marquee {
	/* required styles */
	display: block;
	padding: 0;
	margin: 0;
	list-style: none;
	line-height: 1;
	position: absolute;
	overflow: hidden;
	z-index:1000;

	/* optional styles for appearance */
	left:0px;
	top:8px;
	width: 640px;
	height: 19px; /* height should be included to reserve visual space for the marquee */

	background-color: #660000;
	border: none
}

ul.marquee li {
	/* required styles */
	position: absolute;
	top: 1em;
	left: 0;
	display: block;
	white-space: nowrap; /* keep all text on a single line */

	/* optional styles for appearance */
	font: 14px Arial, Helvetica, sans-serif;
	font-style: italic;
	color: #fff;
	padding: 3px 5px;
}

