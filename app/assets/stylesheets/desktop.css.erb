@charset "UTF-8";
/* CSS Document */

/* Clear Fix to prevent div collapse */
.clearfix:before,
.clearfix:after {
    content:"";
    display:table;
}
.clearfix:after {
    clear:both;
}

.clearfix {
    zoom:1;
}
/* Clear Fix end */

