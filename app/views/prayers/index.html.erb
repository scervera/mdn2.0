<div class="on_white"><div class="pagination"<%= will_paginate @prayers %></div></br>	<h1>Listing prayers</h1></br>	<table>  		<tr>    		<th width="15%" height="38">First name</th>    		<th width="20%">Last name</th>    		<th width="50%">Prayer Request</th>    		<th width="5%">Moderated</th>    		<th width="10%" nowrap="nowrap"> Date Submitted</th>  		</tr>		<% @prayers.each do |prayer| %>  		<tr class="data">    		<td><%= prayer.first_name %></td>    		<td><%= prayer.last_name %></td>    		<td width="50%"><%= prayer.request_original %></td>			<td><%= prayer.moderated %></td>			<td nowrap="nowrap"><%= prayer.created_at.strftime("%b. %d, %Y") %></td>    		<td><%= link_to 'Moderate', edit_prayer_path(prayer) %></td>    		<td><%= button_to 'Delete', { :action => "destroy", :id => prayer.id }, :confirm => 'Are you sure?', :method => :delete %></td>  		</tr>		<% end %>	</table></br><div class="pagination"<%= will_paginate @prayers %></div></div>